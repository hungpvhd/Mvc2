@model T1708E_Website.Models.Student
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit Student</h2>
<form action="Update" method="post">
    <input type="hidden" name="id" value="@Model.Id" />
    <div>Username <input name="name" value="@Model.Name"/></div>
    <div>Rollnumber <input name="rollNumber" value="@Model.RollNumber"/></div>
    <div>
        <input type="submit" value="Submit"/>
        <input type="reset" value="Reset"/>
    </div>
</form>


